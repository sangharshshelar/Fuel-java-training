package com.example.webdemo;

import org.springframework.stereotype.Controller;

@Controller
public class StudentDetails {
	
	private Student stud;
	
	
	void setData()
	{
		stud.setId(100);
		stud.setName("abc");
		
	}
	
	
	void showData()
	{
		System.out.println(stud.getId());
       System.out.println(stud.getName());
		
	}
	

}
